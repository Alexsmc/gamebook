<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.savegames.continuarjogo.ContinuarJogoController">
   <children>
      <TableView fx:id="listaArquivos" layoutX="1.0" layoutY="3.0" onKeyReleased="#checkSelected" onMouseClicked="#checkSelected" prefHeight="350.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="colunaNomeL" prefWidth="273.0" text="Nome Livro" />
          <TableColumn fx:id="colunaNomeH" prefWidth="247.0" text="Nome Personagem" />
            <TableColumn fx:id="colunaPos" prefWidth="79.0" text="Posicao" />
        </columns>
      </TableView>
      <Button fx:id="buttonContinuar" layoutX="248.0" layoutY="360.0" mnemonicParsing="false" onAction="#carregarArquivo" prefHeight="29.0" prefWidth="115.0" text="Carregar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#voltar" text="&lt;&lt;" />
      <Button fx:id="buttonRemover" layoutX="472.0" layoutY="362.0" mnemonicParsing="false" onAction="#remover" prefHeight="29.0" prefWidth="115.0" text="Remover">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
